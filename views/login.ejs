    <nav class="nav-bar">
        <h1>Home</h1>
        <ul class="nav-options">
            <%- include('links/welcome') %>
            <%- include('links/register') %>
            <%- include('links/login') %>
        </ul>
    </nav>

    <div class="container">
        <div class="userForm" id="userLoginForm">

            <h1 class="title" id="titleLogin">LOGIN</h1>

                <% if(message !== null){%>
                    <div class="return-message visible"> 
                        <%= message %> 
                    </div>
                <% } %>

            <form class="form" action="/signIn" method="post">
                <input type="email" name="username" class="userInput" placeholder="Email@example.com" required>
                <input type="password" name="password" class="userInput" placeholder="Ppassword" required>
                <div class="btn-holder">
                    <Button class="inputBtn" type="submit">Login</Button>
                    <Button class="inputBtn" type="reset">Cancel</Button>
                </div>
                <p>You don't have an account? </p><a href="/auth/register">Register</a>
            </form>
        </div>
    </div>